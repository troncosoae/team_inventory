<h1>Items</h1>
<table>
    <thead>
        <tr>
            <th scope="col">iid</th>
            <th scope="col">name</th>
            <th scope="col">bid</th>
            <th scope="col">insterted_at</th>
            <th scope="col">updated_at</th>
            <th scope="col">update</th>
            <th scope="col">delete</th>
        </tr>
    </thead>
    <tbody>
    <% items.forEach(item => { %>
        <tr>
            <td><%- item.iid %></td>
            <td><%- item.name %></td>
            <td><%- item.bid %></td>
            <td><%- item.inserted_at %></td>
            <td><%- item.updated_at %></td>
            <td><form action="items/update/<%- item.iid %>" method="get"><input type="submit" value="update" /></form></td>
            <td><form action="items/delete/<%- item.iid %>" method="post"><input type="submit" value="delete"/></form></td>
        </tr>
    <% }) %>
    </tbody>
</table>
<form action="items/create" method="get">
    <input type="submit" value="create"/>
</form>